{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "TTD Replay",
      "type": "node",
      "request": "launch",
      "protocol": "inspector",
      "stopOnEntry": true,
      "windows": {
        "runtimeExecutable": "nvs.cmd"
      },
      "osx": {
        "runtimeExecutable": "nvs"
      },
      "linux": {
        "runtimeExecutable": "nvs"
      },
      "runtimeArgs": [
        "run",
        "chakracore/latest",
        "--nolazy",
        "--break-first",
        "--replay-debug=c:\\Chakra\\RFSDemo\\_debug_traces_"
      ],
      "console": "internalConsole"
    }
  ]
}